{
  "name": "mrthito/jetstrap",
  "description": "A Laravel 8 package to easily switch TailwindCSS resources generated by Laravel Jetstream to Bootstrap 4 or 5.",
  "keywords": ["laravel", "bootstrap", "jetstream"],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/mrthito/jetstrap/issues",
    "source": "https://github.com/mrthito/jetstrap"
  },
  "authors": [
    {
      "name": "Anitche Chisom",
      "email": "anitchec.dev@gmail.com",
      "role": "Developer"
    },{
      "name": "Mrthito",
      "email": "info@ipatco.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|^8.0|^8.1|^8.2",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "inertiajs/inertia-laravel": "^0.6",
    "laravel/breeze": "^1.21",
    "laravel/jetstream": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "MrThito\\Jetstrap\\": "src/MrThito/Jetstrap/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MrThito\\Jetstrap\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "MrThito\\Jetstrap\\JetstrapServiceProvider"
      ],
      "aliases": {
        "Jetstrap": "MrThito\\Jetstrap\\JetstrapFacade"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "tests": "vendor/bin/phpunit",
    "testsVerbose": "@tests -vvv"
  }
}
